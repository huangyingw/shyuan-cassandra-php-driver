[remote "origin"]
    url = git@github.com:shyuan/cassandra-php-driver.git
    fetch = +refs/heads/*:refs/remotes/origin/*
[remote "github"]
    url = git@github.com:huangyingw/shyuan-cassandra-php-driver.git
    fetch = +refs/heads/*:refs/remotes/github/*
[push]
    remote = github
[gsync]
    remote = origin
    branch = master
[merge]
	allow = true
[branch "dev"]
    remote = github
    merge = refs/heads/dev
[branch "master"]
    remote = origin
    merge = refs/heads/master
[commit]
	message = n
[gdif]
    ignore = *.gdio.diff
[vrun]
	silent = false
	async = false
[deploy]
	host = localhost
	path = ~/myproject/git/php/shyuan-cassandra-php-driver
